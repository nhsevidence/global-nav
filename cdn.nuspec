<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>NICE.GlobalNav.CDN</id>
        <version>1.0.0</version>
        <title>NICE GlobalNav CDN</title>
        <authors>NICE Digital Services,Ian Routledge</authors>
        <owners>nhsevidence</owners>
        <projectUrl>https://github.com/nhsevidence/global-nav</projectUrl>
        <iconUrl>https://cdn.nice.org.uk/V3/Content/nice/favicon-120.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>CDN Package for Global Nav including minified and development versions of the build plus the sourcemap</description>
        <tags>cdn nav javascript css</tags>
		<dependencies></dependencies>
    </metadata>
    <files>
		<!--
		Latest build goes into the root of the tophat subfolder on the CDN.
		E.g. https://alpha-cdn.nice.org.uk/global-nav/global-nav.min.js
		 -->
		<file src="dist\*.*" target="global-nav" exclude="dist/index.html" />
		<!--
		Deploy specific version to a numbered sub folder.
		This allows other applications to reference a specific version of global nav.
		This may be useful in case of breaking changes within Global Nav.
		E.g. https://alpha-cdn.nice.org.uk/global-nav/0.0.1.1-r1234567/global-nav.min.js
		-->
		<file src="dist\*.*" target="global-nav/$version$/" exclude="dist/index.html" />
	</files>
</package>
