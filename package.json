    "@nice-digital/icons": "^2.1.1",
    "@nice-digital/nds-core": "1.0.12-alpha.0",
    "accessible-autocomplete": "^1.6.2",
    "auth0-js": "^9.12.2",
    "classnames": "^2.2.6",
    "es5-polyfill": "0.0.6",
    "js-cookie": "^2.2.1",
    "nervjs": "^1.5.5",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.12.18",
    "unfetch": "^4.1.0"
  },
  "files": [
    "lib",
    "*.d.ts"
  ],
  "directories": {
    "lib": "lib"
  }
}
