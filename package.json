{
  "name": "@nice-digital/global-nav",
  "version": "1.0.0",
  "description": "Global header and footer components for NICE digital services",
  "main": "lib/index.js",
  "types": "./types.d.ts",
  "scripts": {
    "// LOCAL DEV": "",
    "start": "cross-env NODE_ENV=development webpack-dev-server --https --hot --inline",
    "start:nohot": "cross-env HOT=false NODE_ENV=development webpack-dev-server",
    "// PRODUCTION BUILDS": "",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "es5ify": "babel src -d lib --ignore \"src/*.test.js\",\"src/setupTests.js\",\"src/polyfill/**/*.js\"",
    "copy": "copyfiles --up 1 src/**/*.{scss,json} lib",
    "// TESTS": "",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --notify",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "// LINTING": "",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint **/*.scss",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "// LIFECYCLE HOOKS": "",
    "prepare": "npm run es5ify && npm run copy"
  },
  "author": "NICE Digital Services (https://github.com/nice-digital/global-nav/graphs/contributors)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/)",
    "Ian Routledge (https://www.ediblecode.com)"
  ],
  "homepage": "https://github.com/nice-digital/global-nav",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/global-nav.git"
  },
  "license": "MIT",
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">= 8.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-member-expression-literals": "^7.2.0",
    "@babel/plugin-transform-property-literals": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/plugin-transform-reserved-words": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@hot-loader/react-dom": "^16.11.0",
    "@nice-digital/eslint-config": "^1.0.3",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "copyfiles": "^2.1.0",
    "core-js": "^2.6.10",
    "cross-env": "^6.0.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "es3ify-loader": "^0.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-teamcity-reporter": "^0.9.0",
    "mockdate": "^2.0.5",
    "nerv-devtools": "^1.5.5",
    "node-sass": "^4.13.0",
    "pixrem": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "promise-polyfill": "^8.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^2.2.1",
    "stylelint-scss": "^3.12.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
