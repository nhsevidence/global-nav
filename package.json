{
  "name": "@nice-digital/global-nav",
  "version": "4.0.0",
  "description": "Global header and footer components for NICE digital services",
  "main": "lib/index.js",
  "types": "./types.d.ts",
  "scripts": {
    "// LOCAL DEV": "",
    "start": "webpack serve --mode development --hot --inline --config webpack.config.js",
    "start:nohot": "cross-env HOT=false webpack serve --mode development",
    "// PRODUCTION BUILDS": "",
    "build": "webpack --mode=production --config webpack.config.prod.js",
    "es5ify": "cross-env NODE_ENV=production babel src -d lib --ignore \"src/**/*.test.js\",\"src/**/*.test.jsx\",\"src/setupTests.js\",\"src/polyfill/**/*.js\"",
    "copy": "copyfiles --up 1 \"src/**/*.{scss,json,svg}\" lib",
    "// TESTS": "",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --notify",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "// LINTING": "",
    "lint": "npm run prettier && npm run lint:js && npm run lint:scss",
    "prettier": "prettier --check .",
    "prettier:fix": "npm run prettier -- --write",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint **/*.scss",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "// LIFECYCLE HOOKS": "",
    "prepare": "npm run es5ify && npm run copy"
  },
  "author": "NICE Digital Services (https://github.com/nice-digital/global-nav/graphs/contributors)",
  "contributors": [
    "NICE Digital Services (https://www.nice.org.uk/)",
    "Ian Routledge (https://www.ediblecode.com)"
  ],
  "homepage": "https://github.com/nice-digital/global-nav",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nice-digital/global-nav.git"
  },
  "license": "MIT",
  "browserslist": [
    "extends @nice-digital/browserslist-config"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">= 8.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-member-expression-literals": "^7.12.1",
    "@babel/plugin-transform-property-literals": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "@babel/plugin-transform-reserved-words": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@nice-digital/eslint-config": "^1.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "autoprefixer": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "copyfiles": "^2.4.0",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "es3ify-loader": "^0.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-webpack-plugin": "^2.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-teamcity-reporter": "^0.9.0",
    "mockdate": "^3.0.2",
    "nerv-devtools": "^1.5.7",
    "nervjs": "^1.5.7",
    "pixrem": "^5.0.0",
    "postcss": "^8.2.10",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "promise-polyfill": "^8.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-refresh": "^0.9.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "source-map-loader": "^1.1.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@nice-digital/browserslist-config": "^1.0.0",
    "@nice-digital/icons": "^2.1.7",
    "@nice-digital/nds-core": "1.0.21-alpha.0",
    "accessible-autocomplete": "^2.0.3",
    "classnames": "^2.2.6",
    "es5-polyfill": "0.0.6",
    "js-cookie": "^2.2.1",
    "prop-types": "^15.7.2",
    "unfetch": "^4.2.0"
  },
  "files": [
    "lib",
    "*.d.ts"
  ],
  "directories": {
    "lib": "lib"
  }
}
