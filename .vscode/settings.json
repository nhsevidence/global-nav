{
	"editor.formatOnSave": true,
	"editor.rulers": [
		80
	],
	"files.eol": "\n",
	"sasslint.enable": false,
	"css.validate": false,
	"less.validate": false,
	"scss.validate": false,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"obj": true,
		"bin": true,
		"publish": true,
		"coverage": true,
		"dist": true,
		"lib": true
	}
}
